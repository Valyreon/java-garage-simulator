<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchor" prefHeight="65.0" prefWidth="441.0"
            xmlns="http://javafx.com/javafx/8" fx:controller="net.etfbl.garage.user.NumberDialogController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="329.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Label text="Minimum number (0-28):"/>
        <TextField fx:id="numField" promptText="10" text="10">
            <HBox.margin>
                <Insets bottom="10.0" left="5.0" right="5.0" top="10.0"/>
            </HBox.margin>
        </TextField>
        <Button fx:id="submitButton" defaultButton="true" mnemonicParsing="false" onAction="#handleSubmitButtonAction"
                prefWidth="100.0" text="Submit">
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
        </Button>
    </HBox>
</AnchorPane>
